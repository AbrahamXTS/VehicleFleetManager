pipeline {
    agent any
    stages {
        stage('Copy archive') {
            steps {
                copyArtifacts(projectName: 'proy-devops-back-build')
            }
        }
        stage('Stop active containers') {
            steps {
                sh 'docker compose down'
            }
        }
        stage('Deploy') {
            steps {
                sh 'docker compose up -d'
            }
        }
    }
}